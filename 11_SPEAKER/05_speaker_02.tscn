script/source = "extends Node
@onready var photo_circle = $\"../00_NPC_CREATOR/Npc_convas/Photo_circle\"


var _active : bool
func _ready():
	
	_global_datas.player_active_tools.connect(_active_photo)
	
func _active_photo(condition):
	_active = condition


func _on_button_button_down():
	if !_active:
		return
	_global_datas._take_Npc_photo.emit(true)

	
func _on_button_button_up():
	if !_active:
		return
	_global_datas._take_Npc_photo.emit(false)


func _on_button_mouse_entered():
	if !_active:
		return
	photo_circle.visible = true

func _on_button_mouse_exited():
	if !_active:
		return
	photo_circle.visible = false
"

[sub_resource type="GDScript" id="GDScript_6qc5w"]
script/source = "extends Node

@onready var cam_npcs = $\"../Npc_convas/CAM_NPCS\"

@onready var _material : Material = cam_npcs.get_material()

@onready var timer = $Timer

@export var active_npc_entrance : bool = false

func _ready():
	if !active_npc_entrance:
		return
	_global_datas._open_dialogue.connect(_start)

func _start():
	
	timer.start()
	_material.set_shader_parameter(\"Opacity\",2.0)
	_global_datas._hide_dialogue_box.emit()
	
func _process(delta):
	
	if timer.is_stopped():
		return	
	
	var lerp_time = lerp(1.0,2.0,(timer.time_left / timer.wait_time ))
	_material.set_shader_parameter(\"Opacity\",lerp_time)
"

[sub_resource type="GDScript" id="GDScript_hx3ow"]
script/source = "extends Node


@onready var timer = $Timer
@onready var loader = $\"../Models/Loader\"

@export var active_npc_entrance : bool = false
func _ready():
	if !active_npc_entrance:
		return
	_global_datas._open_dialogue.connect(_start)

func _start():
	
	timer.start()
	#loader.transform.origin.y = -8.0
	_global_datas._hide_dialogue_box.emit()
	
func _process(delta):
	
	if timer.is_stopped():
		return	
	
	#loader.transform.origin.y = lerp(0.0,-8.0,(timer.time_left / timer.wait_time ))
	loader.rotation_degrees.z = lerp(0.0,-60.0,(timer.time_left / timer.wait_time ))

func _on_timer_timeout():
	#loader.transform.origin.y = 0.0
	_global_datas._show_dialogue_box.emit()
"

