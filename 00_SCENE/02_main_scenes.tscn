script/source = "extends Node


@onready var birds = $\"../Birds\"

var t
var is_active = false
func _ready():
	_global_datas._bird_is_comming.connect(_follow)


func _follow():
	
	if is_active:
		return
	
	is_active = true
	var pos = Vector3(_global_datas.player_position.x,birds.global_position.y,_global_datas.player_position.z + 11)
	birds.global_position = pos 
	birds.visible = true
	
	if t:
		t.kill()
	t = create_tween()
	
	t .tween_method(_change_value,0.0,0.01,5.0).set_trans(Tween.TRANS_SINE)	
	await create_tween().tween_interval(5.0).finished
	_end()
	
	

func _change_value(value : float):
	var target = Vector3(_global_datas.player_position.x,birds.global_position.y,_global_datas.player_position.z)
	

	var lerped_position = lerp(birds.global_position, target, value)
	birds.global_position = lerped_position	
	


func _end():
	birds.visible = false

	_global_datas._open_dialogue.emit()
"

