[sub_resource type="GDScript" id="GDScript_xsqte"]
script/source = "extends Node

func _input(event: InputEvent) -> void:
	

	if _global_datas.Player_InMenu:
		return
	if _global_datas.Player_InDialogue:
		return
	

	if _global_datas.Player_InSubScene:
		return
		
		
	if _global_datas._time_state == game_state.time_state.night:
			_global_datas._show_object_legend.emit(true,\"The night is coming\")
			return	

	if event.is_action_released(\"Push_to_subscene\"):
		
	
		_global_datas._active_progress_subscene.emit(false)
		_global_datas._load_subscene.emit(false)

	if event.is_action_pressed(\"Push_to_subscene\"):
	
		if _global_datas.Player_In_Inventory:
			return		
		_global_datas._active_progress_subscene.emit(true)
		_global_datas._load_subscene.emit(true)

"

[sub_resource type="GDScript" id="GDScript_0xo40"]
script/source = "extends Node


@onready var _1 = $\"../--DECOR--/Sonar/01\"
@onready var _2 = $\"../--DECOR--/Sonar/02\"
@onready var sonar = $\"../--DECOR--/Sonar\"

var t
var is_active = false
var lock_tools = false

func _ready():
	_global_datas._active_sonar.connect(active_sonar)
	
	sonar.scale = Vector3(0.1,0.1,0.1)
	_1.visible = false
	_2.visible = false

	
func active_sonar(position : Vector3):
	
	
	
	
	
	sonar.global_position = position
	
	if is_active:
		return
		
	is_active = true
	_1.visible = true
	_2.visible = true
	
	if t:
		t.kill()
	t = create_tween()
	
	t .tween_method(_change_scale,0.0,7.0,1.5).set_ease(Tween.EASE_OUT)
	t.connect(\"finished\",_stop)


func _change_scale(value : float):
	sonar.scale = Vector3(value,value,value)
	
func _stop():
	
	sonar.scale = Vector3(0.1,0.1,0.1)
	_1.visible = false
	_2.visible = false
	is_active = false



"

