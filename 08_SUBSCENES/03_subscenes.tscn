	var lerp_time = lerp(1.0,0.0, timer.time_left / timer.wait_time)	
	material.set_shader_parameter(\"Opacity\",lerp_time)	


func _on_timer_timeout():
	timer.stop()
	subscenes_cam_scene.visible = false
	
"

