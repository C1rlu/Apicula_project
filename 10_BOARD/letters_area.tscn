script/source = "extends Area3D

@export var element : element_data
@export var Note_data : Note_Data
@export var info : info_data
@onready var note_text = $Render_text
@onready var note = $Note

@onready var quest_info = $Quest_letter/Text_quest/text

@onready var root = $\".\"
@export var On_Move : Node
@export var On_Over : Node


func _ready():

	On_Move.On_Move.connect(Open_quest_responce)
	On_Over.On_Over.connect(_show_quest_info)


func _show_quest_info(condition):
	#_global_datas.show_element_info.emit(condition,element.element_name)
	_global_datas._show_object_legend.emit(condition,element.element_name)
	
func Open_quest_responce(condition : bool):
	if condition:
		_global_datas._open_quest_on_board.emit(true)
		#_global_datas._open_letter_quest.emit(true)


"

