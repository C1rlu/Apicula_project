script/source = "extends Node


@onready var color_rect = $\"../ColorRect5/Photo\"
@onready var _material : Material = color_rect.get_material()
@onready var text = $\"../text\"
var t 

var is_denoise = false


func _ready():
	_apply_photo_NPC()
	_global_datas._backFrom_subscene.connect(_change_photo_shade)
	_change_photo_shade()
func _apply_photo_NPC():
	
	if _global_datas.Npc_Dialogue == null:
		return
		
	var imageN = _global_datas.Npc_Dialogue.image
	
	_material.set_shader_parameter(\"Photo\",imageN)
	
	text.text = _global_datas.Npc_Dialogue.name

	
	
func _change_photo_shade():
	
	
	if _global_datas.Player_InSubScene:
		return
	
	if is_denoise:
		return

	is_denoise = true
	
	if t:
		t.kill()
	t = create_tween()
	
	t .tween_method(change_value,1.5,1,1.5).set_trans(Tween.TRANS_SINE)	
	
func change_value(value : float):
	_material.set_shader_parameter(\"Denoiser\" , value)
"

[sub_resource type="GDScript" id="GDScript_7ab2k"]
script/source = "extends Node

@onready var change_photo = $\"../Change_photo\"

var actual_value : float = 1.94

var active = false

func _change_value():
	actual_value -= 0.1
	actual_value = clampf(actual_value,1.0,1.94)
	change_photo.change_value(actual_value)	


func _on_area_2d_area_entered(area):
	
	if !active:
		return
	if !area:
		return
		
	var coin_contact = area.get_node_or_null(\"Contact_coin\")
	if coin_contact:
		_change_value()
"

