[sub_resource type="GDScript" id="GDScript_0a7gw"]
script/source = "extends Node2D


@onready var pin : Array = get_children()


var random_pin : int
var lock_random = false

func _ready():
	_disable_all_pin()	
	
func _random_pin():
	
	# hide all first
	_disable_all_pin()
	
	if !lock_random : 
		random_pin = randi_range(0, pin.size()-1)
		lock_random = true
		
	# show only random number index
	pin[random_pin].visible = true
		
		
func _disable_all_pin():

	for element in pin:
		element.visible = false		
"

[sub_resource type="GDScript" id="GDScript_7y6yw"]
script/source = "extends Node
@onready var photo_square_prefabs = $\"../..\"

@onready var take_object = $\"../../Take_object\"


func _destroy(): 
	
	if _global_datas.Player_InSubScene:
		return
		
	print(\"PHOTO DETROYED\")
	photo_square_prefabs.queue_free()
	
func _in_zone():
	
	if _global_datas.Player_InSubScene:
		return
		
	var a = Color(1.0, 0.25, 0.25, 0.5)
	photo_square_prefabs.set_modulate(a)
	take_object.kill_on_pos = true

func _out_zone():


	var a = Color(1.0, 1.0, 1.0, 1.0)
	photo_square_prefabs.set_modulate(a)
	take_object.kill_on_pos = false

"

